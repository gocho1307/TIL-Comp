(program
    (var c (function (int)
        (println "this should only run once")
        (return 1)
    ))
    (var square (function (void (int el))
        (println (* el el))
    ))

    (var v (objects 5))
    (set (index v 0) 1)
    (set (index v 1) 2)
    (set (index v 2) 3)
    (set (index v 3) 4)
    (set (index v 4) 5)
    (sweep v -> (c) .. 2 with square unless (> 1 2))
    (println "")
    (sweep v -> 3 .. 4 with square unless (> 1 2))

    (return 0)
)